    agent any

    stages {
        stage('Build') {
            steps {
                sh label: '', script: 'mvn clean deploy'
            }
        }
        stage('Test') {
            steps {
                sh label: '', script: 'mvn sonar:sonar'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
